<?xml version="1.0" encoding="UTF-8"?>
<design>
	<namespaces>
		<namespace prefix="gohl" uri="http://www.gohlsolucoes.com.br" />
	</namespaces>
	
	<categories>
		<category id="glib" label="Gohl Lib" defaultExpand="true" />
		<!-- Poderia adicionar mais categorias e organizar os comps por elas, seguindo pelo id da cat. -->
	</categories>
	
	<components>
		<component id="gohl.controls.GButton" name="GButton" category="glib" namespace="gohl" displayName="GButton" >
			<mxmlproperties>
				<textfield attribute="click" label="On click:"/>
			</mxmlproperties>
		</component>
		
		<component id="gohl.controls.GCheckBox" name="GCheckBox" namespace="gohl" category="glib" displayName="GCheckBox" >
			<mxmlproperties>
				<textfield attribute="change" label="On change:"/>
			</mxmlproperties>
		</component>
		
		<component id="gohl.controls.GRadioButton" name="GRadioButton" namespace="gohl" category="glib" displayName="GRadioButton" >
			<mxmlproperties>
				<combo id="selected" name="Selectedadodoo:" />
				<textfield attribute="change" label="On change:"/>
				<combo attribute="selected" label="Selected:"/>
				<combo attribute="super.selected" label="Sup Selected:"/>
				<combo attribute="this.selected" label="this.Selected:"/>
				<combo id="active" name="Active:" />
			</mxmlproperties>
			<defaultattribute name="active" value="true" />
		</component>
		
		<component id="gohl.controls.GClock" name="GClock" namespace="gohl" category="glib" displayName="GClock" >
			<mxmlproperties>
				<textfield id="refreshTime" name="Refresh Time:" />
			</mxmlproperties>
		</component>
		
		<component id="gohl.controls.GSearchTextInput" name="GSearchTextInput" namespace="gohl" category="glib" displayName="GSearchTextInput" >
		</component>
		<component id="gohl.controls.GSimpleSearch" name="GSimpleSearch" namespace="gohl" category="glib" displayName="GSimpleSearch" >
        </component>
	</components>
</design>